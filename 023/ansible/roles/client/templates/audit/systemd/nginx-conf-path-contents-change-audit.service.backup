[Unit]
Description=Check nginx conf path changes
Wants=nginx-conf-path-contents-change-audit.timer

[Service]
EnvironmentFile=/home/vagrant/nginx-conf-path-contents-change-audit.env
Type=oneshot
ExecStart=/home/vagrant/path_contents_change_audit.sh ${AUDIT_PATH}

[Install]
WantedBy=multi-user.target